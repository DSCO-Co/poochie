{
  "name": "next-commerce",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "lint": "next lint",
    "prettier-fix": "prettier --write .",
    "find:unused": "npx next-unused"
  },
  "sideEffects": false,
  "dependencies": {
    "@algolia/recommend": "^4.17.0",
    "@algolia/recommend-js": "^1.8.1",
    "@headlessui/react": "^1.7.13",
    "@radix-ui/react-dropdown-menu": "^1.0.0",
    "@react-spring/web": "^9.4.1",
    "@segment/analytics-next": "^1.51.5",
    "@segment/analytics-node": "1.0.0-beta.26",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/react-gtm-module": "^2.0.1",
    "@upstash/redis": "^1.20.4",
    "@vercel/analytics": "^1.0.0",
    "@vercel/commerce": "workspace:*",
    "@vercel/commerce-bigcommerce": "workspace:*",
    "algoliasearch": "^4.16.0",
    "autoprefixer": "^10.4.2",
    "axios": "^1.3.5",
    "body-scroll-lock": "^4.0.0-beta.0",
    "clsx": "^1.1.1",
    "cookie": "^0.4.1",
    "email-validator": "^2.0.4",
    "flowbite": "^1.6.4",
    "flowbite-react": "^0.4.3",
    "js-cookie": "^3.0.1",
    "keen-slider": "^6.7.0",
    "lodash.random": "^3.2.0",
    "lodash.throttle": "^4.1.1",
    "next": "^13.0.4",
    "next-themes": "^0.2.0",
    "node-fetch": "^3.3.1",
    "postcss": "^8.3.5",
    "postcss-nesting": "^10.1.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-easy-swipe": "^0.0.23",
    "react-fast-marquee": "^1.3.1",
    "react-gtm-module": "^2.0.11",
    "react-icons": "^4.8.0",
    "react-instantsearch-hooks-web": "^6.42.1",
    "react-merge-refs": "^2.0.1",
    "react-use-measure": "^2.1.1",
    "tabbable": "^5.2.1",
    "tailwind-heropatterns": "^0.0.8",
    "tailwindcss": "^3.0.13",
    "universal-cookie": "^4.0.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.8",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash.random": "^3.2.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.6.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "lint-staged": "^13.0.3",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.2.3",
    "prettier": "^2.5.1",
    "ts-loader": "^9.4.2",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}
